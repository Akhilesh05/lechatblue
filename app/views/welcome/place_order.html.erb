<%= form_for :order, url: place_order_path do |f| %>
	<%= f.label :name %><%= f.text_field :name %><br>
	<%= f.label :phone %><%= f.text_field :phone %><br>
	<%= f.label :address %><%= f.text_field :address %><br>
	<%= f.label :city %><%= f.text_field :city %><br>
	<%= f.label :pizza_id %><%= f.text_field :pizza_id %><br>
	<%= f.label :pizza_size %><%= f.text_field :pizza_size %>
	<%= f.submit %>
<% end %>